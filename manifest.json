{
  "manifest_version": 2,
  "name": "Frames Anywhere",
  "version": "1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon16.png",
    "64": "icon16.png"
  }
}
